{
  "name": "@v-highlight/vue",
  "version": "0.0.2",
  "description": "Provide highlight.js instruction operation for Vue",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "homepage": "https://github.com/elonehoo/v-highlight",
  "repository": {
    "type": "git",
    "url": "https://github.com/elonehoo/v-highlight"
  },
  "bugs": "https://github.com/elonehoo/v-highlight/issues",
  "keywords": [
    "highlight.js",
    "Vue.js"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "highlight.js": "^11.8.0",
    "vue": "^3.3.4"
  }
}
